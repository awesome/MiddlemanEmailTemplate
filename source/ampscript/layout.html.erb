	concat "<!--\n"
	concat "%%[\n"

    i = 0;
    data.template[ns].each do |key, value|
        concat i == 0 ? "" : ", "
        concat "var @" + ns + '_' + key
        i = i + 1
    end

	data.template[ns].each do |key, value|
	    concat "set @" + ns + '_' + key + " = \"" + value + "\"\n"
	end

	concat "]%%\n"
	concat "-->"
%>
